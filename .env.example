PORT=3000
APP_BASE_URL=http://localhost:3000

# Allowed browser origins (comma-separated). Leave empty for all.
ALLOWED_ORIGINS=http://localhost:3000

# Recruiter dashboard protection (recommended in production)
RECRUITER_ACCESS_KEY=change_me_recruiter_secret

# AI provider
AI_PROVIDER=gemini
GEMINI_API_KEY=your_gemini_key
AI_MODEL=gemini-2.5-flash

# Voice providers (optional)
ELEVENLABS_API_KEY=
ELEVENLABS_VOICE_NAME=rabz test voice
ELEVENLABS_MODEL_ID=eleven_multilingual_v2
ELEVENLABS_STRICT=1
GEMINI_TTS_MODEL=gemini-2.5-flash-preview-tts
GEMINI_TTS_VOICE_EN=Kore
GEMINI_TTS_VOICE_UR=Kore

# Interview speed profile: balanced | ultra_fast | accuracy_first
INTERVIEW_SPEED_PROFILE=balanced

# API rate limiting
RATE_WINDOW_MS=60000
RATE_MAX_DEFAULT=160
RATE_MAX_RESPOND=90
RATE_MAX_TTS=90
RATE_MAX_CREATE=24

# Telemetry logging
ENABLE_LATENCY_TELEMETRY=1
